<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Amazon.Extensions.Configuration.SystemsManager" Version="6.2.1" />
    <PackageReference Include="AWSSDK.S3" Version="3.7.400.59" />
    <PackageReference Include="AWSSDK.SecretsManager" Version="3.7.401.11" />
    <PackageReference Include="AWSSDK.SimpleSystemsManagement" Version="3.7.400.69" />
    <PackageReference Include="AWS.Logger.AspNetCore" Version="3.5.0" />
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="8.0.8" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.8" />
    <PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="8.0.4" />
    <PackageReference Include="StackExchange.Redis" Version="2.8.24" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.7.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="../../migrations/*.sql" CopyToOutputDirectory="PreserveNewest" Link="migrations/%(Filename)%(Extension)" />
  </ItemGroup>
</Project>
