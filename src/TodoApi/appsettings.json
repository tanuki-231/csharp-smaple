{
  "ConnectionStrings": {
    "PostgreSQL": "Host=localhost;Port=5432;Database=tododb;Username=todo_user;Password=todo_pass"
  },
  "Redis": {
    "Connection": "localhost:6379"
  },
  "Session": {
    "TTLMinutes": 20
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000"]
  },
  "AWS": {
    "Enabled": false,
    "Region": "ap-northeast-1",
    "SsmPrefix": "/todo-api/",
    "DbSecretId": "todo-api/db",
    "LogGroup": "/ecs/todo-api"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
